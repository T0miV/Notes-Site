{
  "version": 2,
  "builds": [
    {
      "src": "src/api/vercel.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/src/api/vercel.ts"
    }
  ],
  "headers": [
    {
      "source": "/favicon.ico",
      "headers": [
        {
          "key": "Content-Security-Policy",
          "value": "default-src 'self'; img-src 'self' https://notes-app-oppari-backend.vercel.app"
        }
      ]
    }
  ]
}
